{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "EliseAI Vendor API Docs",
  "openapi": "https://api.eliseai.com/openapi.json",
  "colors": {
    "primary": "#7639fa",
    "light": "#c2b9ff",
    "dark": "#190a4b"
  },
  "favicon": "/favicon.svg",
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": [
        "curl",
        "python",
        "javascript"
      ],
      "defaults": "required"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "quickstart",
              "webhooks",
              "rate-limiting"
            ]
          },
          {
            "group": "Data Model",
            "pages": [
              "data-model/leases",
              "data-model/ledger"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "public": true,
        "url": "api-reference",
        "openapi": "./full-eliseopenapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://www.eliseai.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/eliseai-logo.png",
    "dark": "/logo/eliseai-logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}